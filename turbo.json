{
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "test": {
      "dependsOn": [
        "^build"
      ],
      "outputs": []
    },
    "lint": {
      "outputs": [],
      "parallel": true
    },
    "dev": {
      "cache": false
    },
    "typecheck": {
      "dependsOk": ["^build"],
      "outputs": []
    }
  }
}